{
  "name": "studentloankillerbot",
  "version": "1.0.0",
  "description": "Automated algorithmic trading bot",
  "main": "main.js",
  "scripts": {
    "": "",
    "cleanPreviousImage": "node -e 'require(\"./utils/ecr_handler.js\").cleanPrevImage()'",
    "fetchECRAuthToken": "$(aws ecr get-login --no-include-email --region us-west-2)",
    "buildImage": "docker build -t tradingcluster:tbp -f ./strategies/TBP/Dockerfile .",
    "tagImage": "docker tag tradingcluster:tbp ***REMOVED***",
    "pushImage": "docker push ***REMOVED***",
    "buildTagPushImage": "npm run fetchECRAuthToken && npm run buildImage && npm run tagImage && npm run pushImage",
    "cleanPreviousStack": "node -e 'require(\"./utils/cfn_handler.js\").cleanPrevStack()'",
    "deployTBPService": "aws cloudformation deploy --template-file ./strategies/TBP/tbp_service_cfn.yaml --stack-name TBP --capabilities CAPABILITY_IAM --region us-west-2",
    "startLocalWS": "node -e 'require(\"./utils/localDev.js\").startWS()'",
    "testMinuteAggs": "node --inspect-brk -e 'require(\"./utils/wsClient\").connect()'",
    "tradeTBPPaper": "export trade_env=Paper && node -e 'require(\"./strategies/TBP/3and4barPlay.js\").execute()'",
    "tradeTBPLive": "export trade_env=Live && node -e 'require(\"./strategies/TBP/3and4barPlay.js\").execute()'",
    "test": "node -e 'require(\"./main.js\").test()'",
    "testOutBoundConnect": "curl https://www.google.com && echo this is a test message"
  },
  "keywords": [
    "Trading",
    "Node.js",
    "Stocks",
    "API"
  ],
  "author": "Brock Wade",
  "repository": {
    "type": "git",
    "url": "https://github.com/brockwade633/trading_bot"
  },
  "license": "ISC",
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^1.0.3",
    "aws-sdk": "^2.504.0",
    "lodash": "^4.17.15",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "ws": "^7.2.3"
  }
}
